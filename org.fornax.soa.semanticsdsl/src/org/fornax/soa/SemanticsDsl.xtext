grammar org.fornax.soa.SemanticsDsl with org.eclipse.xtext.common.Terminals

generate semanticsDsl "http://www.fornax.org/soa/SemanticsDsl"

SemanticsModel :
	(imports+=Import)*
	(tagDefinition+=Tag)*;
	
Import :
	'import' importURI=STRING;
	
Tag hidden(WS): 'tag' name=ID ('extends' (extends+=[Tag])*)? ';';
