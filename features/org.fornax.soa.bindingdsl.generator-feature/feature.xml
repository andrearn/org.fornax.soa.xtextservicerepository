   <description url="http://www.eclipse.org/modeling/m2t">
      %description
   </description>

   <copyright url="http://www.eclipse.org/legal/epl-v10.html">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.xtext"/>
      <import plugin="org.fornax.soa.environmentdsl" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.fornax.soa.servicedsl" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.xtext.util"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.emf.common"/>
      <import plugin="org.antlr.runtime"/>
