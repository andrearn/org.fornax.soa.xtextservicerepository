<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<!-- adding resource factories -->


   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.xkonnex.repo.dsl.bindingdsl.ui.newWizards"
            name="Service Repository">
      </category>

      <wizard
            category="org.xkonnex.repo.dsl.bindingdsl.ui.newWizards"
            class="org.xkonnex.repo.dsl.bindingdsl.servicerepo.ui.ServiceRepositoryExecutableExtensionFactory:org.xkonnex.repo.dsl.bindingdsl.servicerepo.ui.wizards.ServiceRepositoryNewProjectWizard"

            id="org.xkonnex.repo.dsl.bindingdsl.servicerepo.ui.wizards.ServiceRepositoryNewProjectWizard"
            name="Service Repository Project"
            project="true">
      </wizard>
    </extension>
    <extension point="org.eclipse.search.searchPages">
	   <page id="org.xkonnex.repo.dsl.bindingdsl.servicerepo.ui.search.dialog.ServiceRepositorySearchPage"
			icon="icons/full/etool16/search.gif"
			label="Service Repository Search"
			extensions="svcdsl:99,moddsl:90,binddsl:90,soldsl:90,sladsl:90,profdsl:90,envdsl:90,semdsl:90"
			showScopeSection="true"
			canSearchEnclosingProjects="true"
			class="org.xkonnex.repo.dsl.bindingdsl.servicerepo.ui.search.dialog.ServiceRepositorySearchPage">
		</page>
	</extension>
	<extension
		id="ServiceRepositorySearchViewPage"
		point="org.eclipse.search.searchResultViewPages">
		<viewPage
			id="org.xkonnex.repo.dsl.bindingdsl.servicerepo.ui.search.dialog.ServiceRepositorySearchViewPage"
			searchResultClass="org.xkonnex.repo.dsl.bindingdsl.servicerepo.ui.search.EObjectSearchResult"
			class="org.xkonnex.repo.dsl.bindingdsl.servicerepo.ui.search.dialog.ServiceRepositorySearchViewPage">
		</viewPage>
	</extension>


</plugin>
