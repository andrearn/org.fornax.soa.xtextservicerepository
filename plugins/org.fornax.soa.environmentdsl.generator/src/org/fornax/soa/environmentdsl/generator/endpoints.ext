import environmentDsl;

String getSOAPHttpEndpointUrl (Server s) : s.getSOAPHttpEndpoint() != null ? ("http://" + s.host.fqn + ":" + s.getSOAPHttpEndpoint().port.toString()) :  null;
String getSecuredSOAPHttpEndpointUrl (Server s) : s.getSOAPHttpEndpoint() != null ? ("https://" + s.host.fqn + ":" + s.getSOAPHttpEndpoint().port.toString()) :  null;

SOAPHTTP getSOAPHttpEndpoint (Server s) : null;
cached SOAPHTTP getSOAPHttpEndpoint (AppServer s) : s.connectors.typeSelect (SOAPHTTP).first();
cached SOAPHTTP getSOAPHttpEndpoint (ESB s) : s.connectors.typeSelect (SOAPHTTP).first();
